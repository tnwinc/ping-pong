{
  "name": "ping-pong-client",
  "version": "0.0.1",
  "devDependencies": {
    "browserify": "~4.1.5",
    "browserify-shim": "~3.5.0",
    "coffee-script": "~1.7.1",
    "coffeeify": "~0.6.0",
    "connect": "~2.18.0",
    "ember-precompile": "~0.2.4",
    "gulp": "~3.6.2",
    "gulp-util": "~2.2.14",
    "through2": "~0.4.2",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1"
  },
  "dependencies": {
    "handlebars": "~1.1.2",
    "jquery": "~2.1.1"
  },
  "browser": {
    "ember": "./src/scripts/vendor/ember.js"
  },
  "browserify-shim": {
    "ember": {
      "exports": "Ember",
      "depends": [
        "jquery:jQuery",
        "handlebars:Handlebars"
      ]
    }
  },
  "scripts": {
    "dev": "gulp dev --require coffee-script/register",
    "clean": "gulp clean --require coffee-script/register",
    "prod": "gulp prod --require coffee-script/register",
    "deploy": "gulp deploy --require coffee-script/register"
  }
}
